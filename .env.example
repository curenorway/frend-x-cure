# Authentication (Required for production)
AUTH_USERNAME=admin
AUTH_PASSWORD=your_secure_password_here

# Storyblok Configuration
STORYBLOK_TOKEN=your_personal_access_token_here
STORYBLOK_SPACE_ID=139140

# Webflow Configuration
WEBFLOW_API_TOKEN=your_webflow_api_token_here
WEBFLOW_SITE_ID=your_webflow_site_id_here

# Note: Get Webflow API token from: https://webflow.com/dashboard/account/integrations
# Get Site ID from: Project Settings → General → Site ID

# Migration Settings
DRY_RUN=true
LOG_LEVEL=info
BATCH_SIZE=50
CONCURRENT_WORKERS=5

# Asset Optimization
IMAGE_QUALITY=85
MAX_IMAGE_WIDTH=2400
MAX_IMAGE_HEIGHT=2400
CONVERT_PNG_TO_WEBP=true
